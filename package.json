{
  "name": "blockly-monaco-editor",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:run": "vitest run",
    "test:contracts": "vitest run src/test/contracts",
    "test:performance": "vitest run src/test/performance",
    "test:watch": "vitest --watch",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint \"./src/**/*.{ts,tsx,vue}\"",
    "lint:fix": "eslint \"./src/**/*.{ts,tsx,vue}\" --fix"
  },
  "dependencies": {
    "@types/mathjs": "^9.4.1",
    "blockly": "^12.2.0",
    "lodash": "^4.17.21",
    "mathjs": "^14.6.0",
    "monaco-editor": "^0.52.2",
    "splitpanes": "^4.0.4",
    "vue": "^3.5.18"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.20",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "@vue/runtime-dom": "^3.5.18",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-vue": "^10.4.0",
    "happy-dom": "^18.0.1",
    "jsdom": "^26.1.0",
    "typescript": "~5.8.3",
    "vite": "^7.0.6",
    "vite-plugin-monaco-editor-esm": "^2.0.2",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.0.4"
  }
}
